<?xml version="1.0"?>
<config>
    <modules>
        <Marktfeed_Api>
            <version>0.2.0</version>
        </Marktfeed_Api>
    </modules>
    <admin>
        <routers>
            <admin_marktfeedapi>
                <use>admin</use>
                <args>
                    <module>Marktfeed_Api</module>
                    <frontName>admin_marktfeedapi</frontName>
                </args>
            </admin_marktfeedapi>
        </routers>
    </admin>
	<adminhtml>
		<acl>
			<resources>
				<all>
					<title>Allow Everything</title>
				</all>
				<admin>
					<children>
						<system>
							<children>
								<config>
									<children>
										<marktfeed>
											<title>Marktfeed - All</title>
										</marktfeed>
									</children>
								</config>
							</children>
						</system>
					</children>
				</admin>
			</resources>
		</acl>
        <translate>
            <modules>
                <Marktfeed_Api>
                    <files>
                        <default>Marktfeed_Api.csv</default>
                    </files>
                </Marktfeed_Api>
            </modules>
        </translate>		
	</adminhtml>
    <global>
        <models>		
            <marktfeed_api>
                <class>Marktfeed_Api_Model</class>
            </marktfeed_api>
        </models>
        <blocks>
            <marktfeed_api>
                <class>Marktfeed_Api_Block</class>
            </marktfeed_api>
        </blocks>
        <helpers>
            <marktfeed_api>
                <class>Marktfeed_Api_Helper</class>
            </marktfeed_api>
        </helpers>
    </global>
</config>